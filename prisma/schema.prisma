generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model User {
  id       Int      @id @default(autoincrement())
  name     String?  @db.VarChar(64)
  email    String?  @db.VarChar(64)
  password String?  @db.VarChar(64)
  language String?  @db.VarChar(64)
  say_name Boolean?
}
